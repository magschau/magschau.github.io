<?xml version="1.0" encoding="UTF-8"?>
<cinematic>

	<frame id="1" loop="true">
		
		<asset>outro_frame1</asset>
		<messages>
			<message character="SpongeBob" asset="spongebob">Barnacles! I got the hidden treasure!</message>
			<message character="SpongeBob" asset="spongebob">I can save the Krusty Krab.</message>
		</messages>
		
   </frame>

   <frame id="2" loop="false">
		
		<asset>outro_frame2</asset>
		<messages>
			<message character="SpongeBuck" asset="spongebuck">Howdy! The name’s SpongeBuck SquarePants.</message>
		</messages>
		
   </frame>

   <frame id="3" loop="true">
		
		<asset>outro_frame4</asset>
		<messages>
			<message character="SpongeBuck" asset="spongebuck">I knew that my Great- Great- Great- Great- Great-</message>
			<message character="SpongeBuck" asset="spongebuck">Great- Great- Grandson would need me one day.</message>
			<message character="SpongeBuck" asset="spongebuck">I dealt with the wily varmint, Dead-Eye Plankton - now you’re dealing with his kin.</message>
			<message character="SpongeBuck" asset="spongebuck">He must be stopped!</message>
			<message character="SpongeBob" asset="spongebob">You're right! But how SpongeBuck?</message>
		</messages>
		
   </frame>

  <frame id="4" loop="false">
		
		<asset>outro_frame5</asset>
		<messages>
			<message character="SpongeBuck" asset="spongebuck">Take this magnificent smelling jellyfish oil to the statue in Dead Eye Gulch</message>
			<message character="SpongeBuck" asset="spongebuck">and give it the biggest clean your spongey, squidgey self can handle.</message>
		</messages>
		
   </frame>

  <frame id="5" loop="true">
		
		<asset>outro_frame6</asset>
		<messages>
			<message character="SpongeBob" asset="spongebob">You mean the jellyfish poop statue in Bikini Bottom?!?</message>
			<message character="SpongeBuck" asset="spongebuck">Believe me boy, there’s more to that statue than meets the eye.</message>
		</messages>
		
   </frame>

   <frame id="6" loop="true">
		
		<asset>outro_frame8</asset>
		<messages>
			<message character="SpongeBob" asset="spongebob">Ah shrimp! This is worse than spring cleaning at the Krusty Krab!</message>
		</messages>
		
   </frame>

    <frame id="7" loop="false">
		
		<asset>outro_frame9</asset>
		<messages>
			<message character="SpongeBob" asset="spongebob">A golden statue that looks like me. Magnificent!</message>
		</messages>
		
   </frame>

   <frame id="8" loop="false">
		
		<asset>outro_frame10</asset>
		<messages>
			<message character="SpongeBob" asset="spongebob">Wait! The treasure! I found the treasure!</message>
			<message character="SpongeBob" asset="spongebob">It really is the treasure this time Mr Krabs, look!</message>
		</messages>
		
   </frame>

   <frame id="9" loop="true">
		
		<asset>outro_frame12</asset>
		<messages>
			<message character="Mr Krabs" asset="mrkrabs">The Krusty Krab is saved thanks to SpongeBob!</message>
			<message character="Pearl" asset="pearl">Yay! Daddy, now can I get those new shoes?</message>
		</messages>
		
   </frame>

   <frame id="10" loop="false">
		
		<asset>outro_frame13</asset>
		<messages>
			<message character="Plankton" asset="plankton">This isn’t over!</message>
			<message character="Plankton" asset="plankton">The Krusty Krab and Krabby Patty recipe will be mine one day!</message>
			<message character="Plankton" asset="plankton">I’ll squeeze that little Sponge next time, just you wait...</message>
		</messages>
		
   </frame>

</cinematic>